  "dependencies": {
    "baseui": "6.17.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.1",
    "styletron-engine-atomic": "1.1.2",
    "styletron-react": "5.0.1"
